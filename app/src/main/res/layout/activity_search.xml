    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="@drawable/background"
        tools:context="com.example.hyoju.dontsick.MainActivity">

    <LinearLayout
        android:layout_width="400dp"
        android:layout_height="70dp"
        android:layout_gravity="center"
        android:gravity="center">
        <Button
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/menu_btn"
            android:layout_marginRight="20dp"/>
        <ImageView
            android:layout_width="250dp"
            android:layout_height="70dp"
            android:gravity="center"
            android:layout_gravity="center"
            android:background="@drawable/where"
            android:layout_marginTop="8dp"
            android:layout_marginRight="30dp"/>
    </LinearLayout>

    <ImageView
        android:layout_width="300dp"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:background="@drawable/dontsick"/>
    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="280dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical">
        <Button
            android:id="@+id/head"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/head"
            android:layout_gravity="center"
            android:text="머리"
            android:gravity="center"
            />
        <LinearLayout
            android:layout_width="300dp"
            android:layout_height="95dp"
            android:layout_gravity="center"
            android:gravity="center">

            <Button
                android:id="@+id/arm1"
                android:layout_width="35dp"
                android:layout_height="70dp"
                android:layout_gravity="center"
                android:background="@drawable/lefthand"
                android:gravity="center"
                android:text="팔"
                android:layout_marginBottom="6dp"/>
            <LinearLayout
                android:layout_width="50dp"
                android:layout_height="100dp"
                android:orientation="vertical">
                <Button
                    android:id="@+id/chest"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:text="가슴"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@drawable/chest" />
                <Button
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:text="배"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@drawable/stomach"
                    android:paddingBottom="10dp"/>
            </LinearLayout>

            <Button
                android:id="@+id/arm2"
                android:layout_width="35dp"
                android:layout_height="70dp"
                android:text="팔"
                android:layout_gravity="center"
                android:gravity="center"
                android:background="@drawable/righthand"
                android:layout_marginBottom="6dp"/>
        </LinearLayout>

        <Button
            android:layout_width="50dp"
            android:layout_height="80dp"
            android:text="다리"
            android:layout_gravity="center"
            android:gravity="center"
            android:background="@drawable/leg"/>

    </LinearLayout>

    <Button
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:background="@drawable/next_btn"
        android:layout_marginTop="15dp"/>
    <Button
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:background="@drawable/search_disease"
        android:layout_marginTop="5dp"/>
    </LinearLayout>
